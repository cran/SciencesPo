year <- sub("-.*", "", meta$Date) 
note <- sprintf("R package version %s", meta$Version) 
bibentry(bibtype = "Manual", title = "{SciencesPo}: A tool set for political behaviour data", author = person("Daniel", "Marcelino"), year = year, note = note, url = "http://CRAN.R-project.org/package=SciencesPo")
